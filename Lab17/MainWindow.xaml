<Window x:Class="Lab17.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab17"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" MouseMove="Window_MouseMove">
    <Canvas x:Name="Canvas">
        <Button x:Name="OkButton" Content="OK" HorizontalAlignment="Left" Margin="323,200,0,0" VerticalAlignment="Top" Width="119" Height="45" MouseEnter="button1_MouseEnter" Click="button1_Click"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="323,250,0,0" VerticalAlignment="Top" Width="119" Height="45" Click="button2_Click"/>
    </Canvas>
</Window>